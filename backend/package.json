{
  "name": "tcf_backend",
  "version": "1.0.0",
  "description": "Express backend for TCF Project",
  "author": "see repository",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsc && node dist/server.js",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.16.1",
    "helmet": "^7.0.0",
    "mongoose": "^7.1.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.2",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}
